      <view class="nav-title">风险评估</view>
      <view class="nav-name">Risk Assessment</view>
      <text class='cuIcon-evaluate_fill'></text>
    </view>
    <view open-type="navigate" hover-class='none'  class="nav-li bg-olive" bindtap='othersTap'>
      <view class="nav-title">其他</view>
      <view class="nav-name">Others</view>
      <text class='cuIcon-icon'></text>
    </view>
  </view>
  <view class='cu-tabbar-height'></view>
